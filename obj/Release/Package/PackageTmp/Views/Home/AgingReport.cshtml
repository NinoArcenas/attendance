                                                {
                                                    @String.Format("{0:MMM dd, yyyy}", Assreaprojected)
                                                }
                                            </label>
                                        </td>
                                        <td class="center">
                                            <label>
                                                @{
                                                    if (cis.date_released_assessors != null)//Actual sa ASSESSOR RELEASE
                                                    {
                                                        @String.Format("{0:MMM dd, yyyy}", cis.date_released_assessors)
                                                    }
                                                }
                                            </label>
                                        </td>
                                        <td style="text-align:center" hidden>
                                            <label>
                                                @{
                                                    DateTime date1;
                                                    DateTime date2;
