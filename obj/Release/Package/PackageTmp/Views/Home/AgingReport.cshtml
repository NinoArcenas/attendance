                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;
                                                    int days90 = 1;
                                                    if (-daysDifference > 0 && -daysDifference < 90)
                                                    {
                                                        @days90
                                                        ninetydays++;
                                                    }
                                                }
                                                else
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;
                                                    int days90 = 1;
                                                    if (-daysDifference > 0 && -daysDifference < 90)
                                                    {
                                                        @days90
                                                        ninetydays++;
                                                    }
                                                }
                                            </label>
                                        </td>
                                        <td style="text-align:center" hidden>
                                            <label>
                                                @if (cis.financing == "HDMF")//91-180 days
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days91_180 = 1;


                                                    if (-daysDifference > 91 && -daysDifference < 180)
                                                    {
                                                        @days91_180
                                                        ninetydays++;
                                                    }
                                                }
                                                else
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;
                                                    int days91_180 = 1;
                                                    if (-daysDifference > 91 && -daysDifference < 180)
                                                    {
                                                        @days91_180
                                                        oneeighty++;
                                                    }
                                                }
                                            </label>
                                        </td>
                                        <td style="text-align:center" hidden>
                                            <label>
                                                @if (cis.financing == "HDMF")//181-365 days
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days180_365 = 1;

                                                    if (-daysDifference > 181 && -daysDifference < 365)
                                                    {
                                                        @days180_365
                                                        treesixfive++;
                                                    }
                                                }
                                                else
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days180_365 = 1;

                                                    if (-daysDifference > 181 && -daysDifference < 365)
                                                    {
                                                        @days180_365
