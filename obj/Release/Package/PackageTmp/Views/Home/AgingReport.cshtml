                    rows += "<td>" + (rodrealactual || '') + "</td>";
                    rows += "<td>" + (preassprojected || '') + "</td>";
                    rows += "<td>" + (preassactual || '') + "</td>";
                    rows += "<td>" + (assreaprojected || '') + "</td>";
