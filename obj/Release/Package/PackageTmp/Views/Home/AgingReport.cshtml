                                                    {
                                                        date1 = Assreaprojected;
                                                        date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                        TimeSpan difference = date1 - date2;
                                                        int daysDifference = difference.Days;
                                                        if (cis.date_released_assessors != null)
                                                        {
                                                            @daysDifference
                                                        }
                                                    }
                                                    else
                                                    {
                                                        date1 = Assreaprojected;
                                                        date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                        TimeSpan difference = date1 - date2;
                                                        int daysDifference = difference.Days;
                                                        if (cis.date_released_assessors != null)
                                                        {
