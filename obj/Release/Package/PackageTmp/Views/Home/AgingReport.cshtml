                                'Please clicked the button!',
                                'error'
                            );
                        }
                    });
                }
                else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Dont empty this field!',
                    }).then(function () {
                        if ($("#year").val() == "") {
                            document.getElementById("warningfieldyear").style.display = "block";
                        }
                    })
                }
            });




            //console
            const data = [
                'July 11, 2023',
                'July 18, 2023',
                'September 2, 2024',
                'July 26, 2024',
                'September 12, 2024',
                'July 26, 2023',
                'January 1, 2023'
            ];

            const months = [
                'January', 'February', 'March', 'April', 'May', 'June',
                'July', 'August', 'September', 'October', 'November', 'December'
            ];

            const countDates = (data) => {
                const count = {};

                data.forEach(date => {
                    const [month, day, year] = date.split(/[\s,]+/);
                    const key = `${year}-${month}`;

                    if (!count[key]) {
                        count[key] = 0;
                    }
                    count[key]++;
                });

                return count;
            };

            const generateOutput = (startYear, endYear, counts) => {
                const result = [];

                for (let year = startYear; year <= endYear; year++) {
                    for (let month of months) {
                        const key = `${year}-${month}`;
                        result.push(`${year}\t${month}\t${counts[key] || 0}`);
                    }
                }

                return result;
            };

            const counts = countDates(data);
            const output = generateOutput(2023, 2024, counts);

            console.log(output.join('\n'));


