                                        </td>
                                        <td style="text-align:center" hidden>
                                            <label>
                                                @if (cis.financing == "HDMF")//366-730 days
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days366_730 = 1;


                                                    if (-daysDifference > 366 && -daysDifference < 730)
                                                    {
                                                        @days366_730
                                                        sevintreeziro++;
                                                    }
                                                }
                                                else
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days366_730 = 1;
                                                    if (-daysDifference > 366 && -daysDifference < 730)
                                                    {
                                                        @days366_730
                                                        sevintreeziro++;
                                                    }
                                                }
                                            </label>
                                        </td>
                                        <td style="text-align:center" hidden>
                                            <label>
                                                @if (cis.financing == "HDMF")//>731 days
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days731 = 1;

                                                    if (-daysDifference > 731)
                                                    {
                                                        @days731
                                                        sevintreeone++;
                                                    }
                                                }
                                                else
                                                {
                                                    date1 = Assreaprojected;
                                                    date2 = Convert.ToDateTime(cis.date_released_assessors);
                                                    TimeSpan difference = date1 - date2;
                                                    int daysDifference = difference.Days;

                                                    int days731 = 1;
                                                    if (-daysDifference > 731)
                                                    {
                                                        @days731
                                                        sevintreeone++;
                                                    }
                                                }
                                            </label>
                                        </td>
                                    </tr>
                                }
